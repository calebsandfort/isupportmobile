{
  "name": "ReactNativeSeed",
  "version": "1.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "8.1.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "chai-string": "^1.4.0",
    "eslint": "4.14.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "2.4.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-react-native": "3.2.0",
    "expect": "^22.4.3",
    "flow-bin": "0.62.0",
    "flow-typed": "2.2.3",
    "husky": "0.14.3",
    "jest-expo": "^24.0.0",
    "jest-spec-reporter": "^1.0.3",
    "mocha": "^5.0.5",
    "react-native-scripts": "1.8.1",
    "react-test-renderer": "16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "remote-redux-devtools": "^0.5.0",
    "remotedev-server": "^0.2.4"
  },
  "main": "node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "flow && eslint . && yarn jest",
    "jest": "node node_modules/jest/bin/jest.js",
    "jest:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "precommit": "yarn test",
    "test:mocha": "mocha src/services/*.dev-test.js",
    "test:mocha:watch": "npm run test:mocha -- --watch",
    "react-devtool": "react-devtools",
    "redux-devtool": "remotedev",
    "eslint": "eslint . -f table",
    "native-debugging": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" npm run android",
    "flow:stop": "flow stop"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setupTestFrameworkScriptFile.js",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|lottie-react-native|expo|react-native-maps|react-native-svg|react-native-branch|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|native-base|@expo|react-native-scrollable-tab-view)/)"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "es6-symbol": "^3.1.1",
    "expo": "^24.0.0",
    "i": "^0.3.6",
    "impagination": "^1.0.0-alpha.3",
    "jsencrypt": "^3.0.0-beta.1",
    "lodash": "4.17.4",
    "moment": "^2.22.0",
    "native-base": "2.3.5",
    "qs": "^6.5.1",
    "react": "16.2.0",
    "react-devtools": "^3.2.1",
    "react-native": "0.51.0",
    "react-native-easy-grid": "^0.1.17",
    "react-native-fs": "^2.9.12",
    "react-navigation": "^2.5.3",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-form": "7.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-loop": "^4.2.4",
    "redux-persist": "^4.10.2",
    "redux-persist-expo-securestore": "^1.0.0",
    "redux-persist-filesystem-storage": "^1.2.0",
    "redux-persist-fs-storage": "^1.2.1",
    "redux-thunk": "2.2.0",
    "remote-redux-devtools-on-debugger": "^0.8.0",
    "reselect": "^3.0.1",
    "underscore.string": "^3.3.4",
    "util": "^0.11.0"
  }
}
